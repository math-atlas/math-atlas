   root = Mjoin( PATL, pt@(sh)rk_nt )( ATL_NTHREADS, &attr, UPLO, TRANS,
                                    N, K, (void *)(&alpha0), (void *)(A),
                                    LDA, (void *)(&beta0), (void *)(C), LDC );
#else
   root = Mjoin( PATL, pt@(sh)rk_nt )( ATL_NTHREADS, &attr, UPLO, TRANS,
                                    N, K, (void *)(ALPHA), (void *)(A),
                                    LDA, (void *)(BETA), (void *)(C), LDC );
