   unsigned int               next = ( NODE << 1 ), ntm1, ntm2, ntn1, ntn2;
   int                        incB, incC, m1, m2, n1, n2;
@extract @(EXECUTABLE_STATEMENT)
   if( NTHREADS < 1 ) return( tree );

   size = PLVL3->size;
 
   if( SIDE == AtlasLeft )
   {
