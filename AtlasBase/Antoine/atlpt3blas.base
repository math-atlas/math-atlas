@define rname @Mjoin( PATL, pt@(sh)rk )@
void @(rname)
(
   const enum ATLAS_UPLO      UPLO,
   const enum ATLAS_TRANS     TRANS,
   const int                  N,
   const int                  K,
@rout ATL_ptsyrk
   const SCALAR               ALPHA,
@rout ATL_ptherk
   const TYPE                 ALPHA,
@rout ATL_ptsyrk ATL_ptherk
   const TYPE                 * A,
   const int                  LDA,
@rout ATL_ptsyrk
   const SCALAR               BETA,
