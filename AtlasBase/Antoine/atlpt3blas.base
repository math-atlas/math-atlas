@beginskip
   if( ( ( M <= nb ) && ( N <= nb ) ) || ( ( N <= nb ) && ( K <= nb ) ) ||
       ( ( K <= nb ) && ( M <= nb ) ) || ( NTHREADS <= 1 ) || 
       ( tblks <= (double)(ATL_XOVER_L3_DEFAULT) ) )
@endskip
   if (NOTHR)
   {
#ifdef TREAL
      alpha    = *((TYPE *)(ALPHA));   beta     = *((TYPE *)(BETA));
#else
      alpha[0] = ((TYPE *)(ALPHA))[0]; alpha[1] = ((TYPE *)(ALPHA))[1];
      beta [0] = ((TYPE *)(BETA ))[0]; beta [1] = ((TYPE *)(BETA ))[1];
