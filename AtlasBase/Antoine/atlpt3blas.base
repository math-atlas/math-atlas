@define rname @Mjoin( PATL, pt@(sh)rk_nt )@
PT_TREE_T @(rname)
(
   const unsigned int         NTHREADS,
   pthread_attr_t             * ATTR,
   const enum ATLAS_UPLO      UPLO,
   const enum ATLAS_TRANS     TRANS,
   const int                  N,
   const int                  K,
   const void                 * ALPHA,
   const void                 * A,
   const int                  LDA,
   const void                 * BETA,
   void                       * C,
   const int                  LDC
) 
{
@extract @(ATLAS_PT3_COMMENT)
