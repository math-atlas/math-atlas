   if (incX == 1) sum = Mjoin(PATLU,asum)(N<<1, X, 1);
   else
   {
      for (i=N; i; i--, X += incx) 
      {
         sum += Mabs(*X);
         sum += Mabs(X[1]);
      }
   }
   return(sum);
#endif
}
