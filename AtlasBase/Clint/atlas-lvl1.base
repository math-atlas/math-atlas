#endif
   (const int N, const TYPE *X, const int incX)
{
#ifdef TREAL
   int i;
   TYPE sum=0.0;
   if (incX == 1) for (i=N; i; i--, X++) sum += Mabs(*X);
   else for (i=N; i; i--, X += incX) sum += Mabs(*X);
   return(sum);
