      }
   #else
      for (j=0; j < N; j++, C += ldc)
      {
         const TYPE *a=A+j;
         ATL_UINT i;
         for (i=0; i < M; i++, a += lda)
         #if BETA == 0
            C[i] = *a;
         #elif BETA == 1
            C[i] += *a;
         #elif BETA == -1
            C[i] = *a - C[i];
         #else
            C[i] = beta*C[i] + *a;
         #endif
      }
   #endif
}
