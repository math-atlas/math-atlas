   t0 = time00();
   #ifdef TREAL
      dot = TEST_DOT(N, X, incX, Y, incY);
   #else
      TEST_DOT(N, X, incX, Y, incY, dot);
   #endif
   t1 = time00();

   FA_free(X, FAx, MAx);
   FA_free(Y, FAy, MAy);
   return(t1-t0);
}
