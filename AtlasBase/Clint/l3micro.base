         else
            Mjoin(PATL,sycpy@(up)NB_aX)(M, alpha, A, lda, aB, M);
      #endif
   }
   @endwhile
   #ifdef TCPLX
      oi.a2blk(M, M, oi.ONE, aB, M, rA, aA);
