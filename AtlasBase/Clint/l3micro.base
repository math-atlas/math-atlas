                Mjoin(PATL,sycpyLNB_an): Mjoin(PATL,sycpyLNB_aX);
   #endif
   if (SCALAR_IS_ONE(alpha))
      sycp = Mjoin(PATL,sycpyL_a1);
   else if (SCALAR_IS_NONE(alpha))
      sycp = Mjoin(PATL,sycpyL_an1);
   @endskip
/*
 * Loop over full panels of B
 */
   for (j=0; j < Nb; j++)
   {
      ATL_iptr_t i=j*nlm, k=j*nlk, incW=B*kb - nlm*mb;
      ATL_INT kk;
