   const int iRWoff = mb*MUNU;
   ATL_SZT r;
   int i, ix=-1, mbi;

@skip   GetInfo(TA, &amm_b0, &amm_b1, &amm_bn, &l2a, &r2a);
   for (r=0; r < R; r += NU, x += (NU SHIFT)*ldx)
   {
      int mu, nu = R - r;
      const int DoCopy = (!r && NeedCopyA);
      TYPE *d, *Ac = ((TYPE*)A), *xc = x;
      nu = Mmin(nu, NU);
      mu = Mmin(MU, N);
